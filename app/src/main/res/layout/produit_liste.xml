<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <TextView
        android:id="@+id/prixu"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="  PrixU"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/qte1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/Sous_Total"
        android:layout_width="70dp"
        android:layout_height="0dp"
        android:text="SousTotal"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/prixu"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/ordre"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="44dp"
        android:text="Ordre"
        android:textSize="13sp"
        app:layout_constraintEnd_toStartOf="@+id/libelle"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/libelle"
        android:layout_width="60dp"
        android:layout_height="match_parent"
        android:layout_marginStart="5dp"
        android:text="LibellÃ©"
        android:textSize="13sp"
        app:layout_constraintStart_toEndOf="@+id/ordre"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/qte1"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:text="Qte"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/libelle" />

</androidx.constraintlayout.widget.ConstraintLayout>